{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "execute-api:Invoke"
      ],
      "Resource": [
        "arn:aws:execute-api:*:*:*/*/*",
        "arn:aws:execute-api:eu-west-1:*:*/*/fba/inventory/*",
        "arn:aws:execute-api:eu-west-1:*:*/*/listings/*",
        "arn:aws:execute-api:eu-west-1:*:*/*/products/pricing/*",
        "arn:aws:execute-api:eu-west-1:*:*/*/reports/*",
        "arn:aws:execute-api:*:*:*/*/fba/*",
        "arn:aws:execute-api:*:*:*/*/listings/*",
        "arn:aws:execute-api:*:*:*/*/products/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "execute-api:*"
      ],
      "Resource": [
        "arn:aws:execute-api:*:*:*"
      ]
    }
  ]
}